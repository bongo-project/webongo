!!!
%html{:xmlns => "http://www.w3.org/1999/xhtml"}
  %head
    %meta{:content => "text/html; charset=iso-8859-1", "http-equiv" => "content-type"}/
    %title{"tal:content" => "doctitle"} Bongo Web Administration
    %meta{:content => "text/html; charset=UTF-8", "http-equiv" => "Content-Type"}/
    %link{:href => "/img/bongo-favicon.ico", :rel => "icon", :type => "image/ico"}/
    %link{:href => "/img/bongo-favicon.png", :rel => "icon", :type => "image/png"}/
    %style{:media => "screen", :type => "text/css"}
      @import url(/css/admin.css);
    %script{:src => "/js/lib/prototype/prototype.js", :type => "text/javascript"}
    %script{:src => "/js/lib/script.aculo.us/effects.js", :type => "text/javascript"}
  %body#body-override
    #userinfo
      Logged in as:
      %i admin
      [
      %a.blink{:href => "%(url|/)slogout"}> logout
      ]
    #logo
      %img{:alt => "Bongo", :src => "/img/Bongo-logo.png"}/
    #wrap
      #sidebar-wrap
        #sidebar
          %ul.nav
            %li{"tal:attributes" => "class dsktab"}
              %a{:href => "%(url|/)s"}
                %img.icon{:alt => "", :border => "0", :src => "/img/user-desktop.png"}/
                Desktop
            %li.disabled
              %img.icon{:alt => "", :src => "/img/system-users.png"}/
              Users
            %li{"tal:attributes" => "class agntab"}
              %a{:href => "%(url|/)sagents"}
                %img.icon{:alt => "", :border => "0", :src => "/img/system-installer.png"}/
                Agents
            %li{"tal:attributes" => "class systab"}
              %a{:href => "%(url|/)ssystem"}
                %img.icon{:alt => "", :border => "0", :src => "/img/network-server.png"}/
                System
            %li.disabled
              %img.icon{:alt => "", :src => "/img/help-browser.png"}/
              Help
        %a#sidebartoggle.nlink{:href => "#", :onclick => "toggleSidebar();", :style => "font-size: 70%%;"} Toggle sidebar
      #page
        #content
          / BEGIN HAWKYE CONTENT
